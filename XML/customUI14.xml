<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui">
	<ribbon>
		<tabs>
			<tab idMso="TabDeveloper">
				<group id="AddinControl" label="Add-in Control">
					<button id="ImportCode" label="Import Code from Folder" onAction="ImportCodeSelection" />
					<button id="ExportCode" label="Export Code to Folder" onAction="ExportCodeSelection" />
					<button id="UpdateXML" label="ImportXML" />
					<separator id="BestPractices"/>
					<button id="BestPracticeReport" label="Best Practice Report"/>
					<splitButton id="Debugging">
						<menu id="DebuggingMenu">
							<button id="ToggleDebugging" label="Toggle Debugging"/>
							<button id="AddDebugging" label="Add Debugging to each module in File"/>
						</menu>
					</splitButton>
					<separator id="Filetools"/>
					<button id="ToggleIsAddIn" label="Toggle Is AddIn"/>
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
